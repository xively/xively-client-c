#!/usr/bin/expect -f
set timeout 300
set prompt "(%|#|\\$) $"

eval spawn $argv

expect {
    "Please acknowledge these tool dependencies. Do you wish to proceed?" {
      send -- "Y"
      exp_continue
    }

    "Do you agree to the license of this third party library?" {
      send -- "Y"
      exp_continue
  }
}
